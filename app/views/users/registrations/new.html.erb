<div class="row">
  <div class="col s10 offset-s1">
    <h4>Cadastro</h4>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="input-field col m6">
        <i class="large material-icons prefix">account_circle</i>
        <%= f.text_field :name, class: "validate", require: true %>
        <%= f.label :name  %>
      </div>

      <div class="input-field col m6">
        <i class="large material-icons prefix">email</i>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'validate' %>
        <%= f.label :email,  'data-error' => "Opa! insira um email válido!",  'data-success'=>"Email válido!" %>
      </div>

      <div class="input-field col m6">
        <i class="large material-icons prefix">account_circle</i>
        <%= f.text_field :telephone, class: "validate" %>
        <%= f.label :telephone  %>
      </div>

      <div class="input-field col m6">
        <i class="large material-icons prefix">account_circle</i>
        <%= f.text_field :cell, class: "validate" %>
        <%= f.label :cell  %>
      </div>

      <div class="input-field col m6">
        <i class="large material-icons prefix">fingerprint</i>
        <%= f.password_field :password, autocomplete: "off", class:"validate" %>
        <%= f.label :password %>
      </div>

      <div class="input-field col m6">
        <i class="large material-icons prefix red-text">fingerprint</i>
        <%= f.password_field :password_confirmation, autocomplete: "off", class:"validate" %>
        <%= f.label :password_confirmation %>
      </div>

      <div class="actions">
        <%= f.submit "Cadastrar", class: 'btn' %>
      </div>
    <% end %>
    <br>
    <%= render "users/shared/links" %>
  </div>
</div>
<script>
